# LeeTour Project SSH Configuration
# Use this with: ssh -F .ssh/config leetour

Host leetour
    HostName 157.173.124.250
    User deployer
    Port 22
    ServerAliveInterval 60
    ServerAliveCountMax 3

Host leetour-mongodb
    HostName 157.173.124.250
    User deployer
    LocalForward 27017 localhost:27017
    ServerAliveInterval 60
    ServerAliveCountMax 3
