    <script>

        $(document).ready(function () {
            var owl_hotel = $('.owl-carousel-hotel');
            owl_hotel.owlCarousel({
                stagePadding: 10,
                items: 2,
                loop: true,
                //margin: 20,
                autoplay: true,
                autoplayTimeout: 1000,
                autoplayHoverPause: true
            });
            var owl_tour = $('.owl-carousel-tour');
            owl_tour.owlCarousel({
                stagePadding: 10,
                items: 2,
                loop: true,
                //margin: 20,
                autoplay: true,
                autoplayTimeout: 1000,
                autoplayHoverPause: true
            });
        });

        $('#country-tour option[value=-1]').prop('selected', true);
        // When the user scrolls the page, execute myFunction
        window.onscroll = function () { myFunction() };

        // Get the navbar
        var navbar = document.getElementById("navbar");

        // Get the offset position of the navbar
        var sticky = 200;

        // Add the sticky class to the navbar when you reach its scroll position. Remove "sticky" when you leave the scroll position
        function myFunction() {
            if (window.pageYOffset >= sticky) {
                navbar.classList.add("sticky");
            } else {
                navbar.classList.remove("sticky");
            }
        }

        function searchMain() {
            var searchByCity = $("#selectTypeCity").is(':checked');
            var searchByText = $("#selectTypeKeyword").is(':checked');
            if (searchByCity) {
                var name = $("#city").val();
                if (name == '' || typeof name === "undefined" || name == null) {
                    swal({ title: "Information!", text: "Please select city for your vacation!", confirmButtonText: "OK" });
                    return false;
                } else if (name == "all") {
                    var country = $("#country-tour option:selected").text();;
                    var url = "/" + country + "/key-none/city-all/filter-none/page-1";
                    window.location.href = url;
                } else {
                    var url = "/search/" + name + "/key-none/filter-all/price-all/page-1";
                    window.location.href = url;
                }
            } else if (searchByText) {
                searchByTour();
            }

        }

        function searchByTourPress(e) {
            if (e.keyCode === 13) {
                e.preventDefault(); // Ensure it is only this code that rusn
                searchByTour();
            }
        }

        function searchByTour() {
            var name = $("#search-name").val();
            var url = "/search/all/key-" + name.trim() + "/filter-all/price-all/page-1";
            window.location.href = url;
        }

    </script>
