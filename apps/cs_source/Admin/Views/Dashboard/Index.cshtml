                        </div><!-- Row -->
                        <div class="row">
                            <div class="col-lg-12 col-md-12">
                                <div class="panel panel-white">
                                    <div class="row">
                                        <div class="col-sm-8">
                                            <div class="panel-heading">
                                                <h4 class="panel-title">Booking Stats</h4>
                                            </div>
                                            <div class="panel-body">
                                                <div class="table-responsive project-stats">
                                                    <table class="table">
                                                        <thead>
                                                            <tr>
                                                                <th>#</th>
                                                                <th>Name</th>
                                                                <th>Status</th>
                                                                <th>Total</th>
                                                                <th>Progress</th>
                                                            </tr>
                                                        </thead>
                                                        @if (Model.Orders.Count > 0)
                                                        {
                                                            List<Order> orders = Model.Orders;

                                                            var holding = orders.Where(o => o.Status == (int)Utilities.BookingStatus.Holding).ToList();
                                                            var percentHolding = (int)(0.5f + ((100f * holding.Count) / orders.Count));

                                                            var paid = orders.Where(o => o.Status == (int)Utilities.BookingStatus.Paid).ToList();
                                                            var percentPaid = (int)(0.5f + ((100f * paid.Count) / orders.Count));

                                                            var cancel = orders.Where(o => o.Status == (int)Utilities.BookingStatus.Cancelled).ToList();
                                                            var percentCancel = (int)(0.5f + ((100f * cancel.Count) / orders.Count));

                                                            var refunded = orders.Where(o => o.Status == (int)Utilities.BookingStatus.Refunded).ToList();
                                                            var percentRefunded = (int)(0.5f + ((100f * refunded.Count) / orders.Count));

                                                            var amended = orders.Where(o => o.Status == (int)Utilities.BookingStatus.Amended).ToList();
                                                            var percentAmended = (int)(0.5f + ((100f * amended.Count) / orders.Count));

                                                            var inactive = orders.Where(o => o.Status == (int)Utilities.BookingStatus.Defautl).ToList();
                                                            var percentInactive = (int)(0.5f + ((100f * inactive.Count) / orders.Count));

                                                            <tbody>
                                                                <tr>
                                                                    <th scope="row">1</th>
                                                                    <td>Holding Booking</td>
                                                                    <td><span class="label label-primary">Holding</span></td>
                                                                    <td>@holding.Count bookings</td>
                                                                    <td>
                                                                        <div class="progress progress-sm">
                                                                            <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="@(percentHolding)" aria-valuemin="0" aria-valuemax="100" style="width: @(percentHolding)%">
                                                                            </div>
                                                                        </div>
                                                                    </td>
                                                                </tr>

                                                                <tr>
                                                                    <th scope="row">2</th>
                                                                    <td>Booking Paid</td>
                                                                    <td><span class="label label-success">Paid</span></td>
                                                                    <td>@(string.Format("{0} {1}", paid.Count, paid.Count > 1 ? "bookings" : "booking"))</td>
                                                                    <td>
                                                                        <div class="progress progress-sm">
                                                                            <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="@(percentPaid)" aria-valuemin="0" aria-valuemax="100" style="width: @(percentPaid)%">
                                                                            </div>
                                                                        </div>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <th scope="row">3</th>
                                                                    <td>Booking Cancelled</td>
                                                                    <td><span class="label label-danger">Canceled</span></td>
                                                                    <td>@(string.Format("{0} {1}", cancel.Count, cancel.Count > 1 ? "bookings" : "booking"))</td>
                                                                    <td>
                                                                        <div class="progress progress-sm">
                                                                            <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="@percentCancel" aria-valuemin="0" aria-valuemax="100" style="width: @(percentCancel)%">
                                                                            </div>
                                                                        </div>
                                                                    </td>
                                                                </tr>

                                                                <tr>
                                                                    <th scope="row">4</th>
                                                                    <td>Booking Amended</td>
                                                                    <td><span class="label label-info">Amended</span></td>
                                                                    <td>@(string.Format("{0} {1}", amended.Count, amended.Count > 1 ? "bookings" : "booking"))</td>
                                                                    <td>
                                                                        <div class="progress progress-sm">
                                                                            <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="@percentAmended" aria-valuemin="0" aria-valuemax="100" style="width: @(percentAmended)%">
                                                                            </div>
                                                                        </div>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <th scope="row">5</th>
                                                                    <td>Booking Refunded</td>
                                                                    <td><span class="label label-warning">Refunded</span></td>
                                                                    <td>@(string.Format("{0} {1}", refunded.Count, refunded.Count > 1 ? "bookings" : "booking"))</td>
                                                                    <td>
                                                                        <div class="progress progress-sm">
                                                                            <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="@(percentRefunded)" aria-valuemin="0" aria-valuemax="100" style="width: @(percentRefunded)%">
                                                                            </div>
                                                                        </div>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <th scope="row">6</th>
                                                                    <td>Booking Inactive</td>
                                                                    <td><span class="label label-primary">Inactive</span></td>
                                                                    <td>@(string.Format("{0} {1}", inactive.Count, inactive.Count > 1 ? "bookings" : "booking"))</td>
                                                                    <td>
                                                                        <div class="progress progress-sm">
                                                                            <div class="progress-bar progress-bar-primary" role="progressbar" aria-valuenow="@(percentInactive)" aria-valuemin="0" aria-valuemax="100" style="width: @(percentInactive)%">
                                                                            </div>
                                                                        </div>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        }
                                                        else
                                                        {
                                                            <tbody>
                                                                <tr>
                                                                    <th scope="row">No content for this time.</th>

                                                                </tr>
                                                            </tbody>
                                                        }

                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-4">

                                            <div class="stats-info">
                                                <div class="panel-heading">
